        <classes>
            <class name="AutomationTesting.StudentRegistationForm"></class>
        </classes>
    </test>

    <test name="Login correct">
        <classes>
            <class name="AutomationTesting.LogIN"/>
        </classes>
    </test>
    
    <test name="Marks Entry Eleven Science">
        <classes>
            <class name="AutomationTesting.MarksEntryElevenS"/>
        </classes>
    </test>
    
    <test name="Marks Entry Eleven Arts">
        <classes>
            <class name="AutomationTesting.MarksEntryElevenA"/>
